#include <stdio.h>

int main()
{
  int n, k = 0, i, j, res;

  do
  {   printf("Enter the number: ");
    res = scanf_s("%d", &n);

    while (getchar() != '\n');


    if (res == 1)

      for (i = 1; i <= n; i++)
        for (j = i; j <= n; j++)
          if (i * i * i + j * j * j == n)
            k++;
  } while (res != 1);

  printf("%d way(s)", k);


  return 0;
}
